{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:generate": "ts-node scripts/generate-migration.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "migration:run:prod": "typeorm migration:run -d dist/src/data-source.js",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts",
    "test": "tsx --test src/**/*.test.ts",
    "test:watch": "tsx --test --watch src/**/*.test.ts",
    "test:coverage": "c8 tsx --test src/**/*.test.ts",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:jest": "jest --testRegex=\".*\\.spec\\.ts$\" --rootDir=src",
    "test:all": "tsx --test src/**/*.test.ts && jest --testRegex=\".*\\.spec\\.ts$\" --rootDir=src",
    "test:jest:watch": "jest --testRegex=\".*\\.spec\\.ts$\" --rootDir=src --watch",
    "test:coverage:jest": "jest --testRegex=\".*\\.spec\\.ts$\" --rootDir=src --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.10",
    "@nestjs/platform-express": "^11.1.10",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.28",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.10",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.7",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/sinon": "^21.0.0",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "c8": "^10.1.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "sinon": "^21.0.1",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.test.ts",
      "!**/*.spec.ts",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/coverage/**"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
